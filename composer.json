{
  "name": "contributte/gui-skeleton",
  "description": "GUI skeleton is full featured web app kit based on Nette Framework + Contributte and many useful packages by @f3l1x.",
  "keywords": [
    "php",
    "nette",
    "contributte",
    "sandbox",
    "project",
    "skeleton"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "contributte/application": "^0.5",
    "contributte/bootstrap": "^0.5",
    "contributte/cache": "^0.6",
    "contributte/di": "^0.5",
    "contributte/forms": "^0.5",
    "contributte/forms-bootstrap": "^0.5",
    "contributte/forms-wizard": "^3.1",
    "contributte/http": "^0.4",
    "contributte/security": "^0.4",
    "contributte/utils": "^0.5",
    "contributte/latte": "^0.5",
    "contributte/tracy": "^0.5",
    "contributte/recaptcha": "^3.4"
  },
  "require-dev": {
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": [
        "app/"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
