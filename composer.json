{
  "name": "contributte/gui-skeleton",
  "description": "GUI project skeleton based on Nette Framework (@nette) and Contributte (@contributte) libraries by @f3l1x.",
  "keywords": [
    "php",
    "nette",
    "contributte",
    "sandbox",
    "project",
    "skeleton"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "contributte/application": "^0.6",
    "contributte/bootstrap": "^0.6",
    "contributte/cache": "^0.6",
    "contributte/di": "^0.5",
    "contributte/forms": "^0.6",
    "contributte/forms-bootstrap": "^0.6",
    "contributte/forms-wizard": "^3.1",
    "contributte/http": "^0.4",
    "contributte/security": "^0.5",
    "contributte/utils": "^0.6",
    "contributte/latte": "^0.6",
    "contributte/tracy": "^0.6",
    "contributte/recaptcha": "^4.0"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/dev": "^0.5",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": [
        "app/"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
