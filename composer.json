{
  "name": "contributte/gui-skeleton",
  "description": "GUI project skeleton based on Nette Framework (@nette) and Contributte (@contributte) libraries by @f3l1x.",
  "keywords": [
    "php",
    "nette",
    "contributte",
    "sandbox",
    "project",
    "skeleton"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.2",

    "contributte/application": "^0.6",
    "contributte/bootstrap": "^0.6",
    "contributte/cache": "^0.6",
    "contributte/di": "^0.5",
    "contributte/forms": "^0.6",
    "contributte/forms-bootstrap": "^0.7",
    "contributte/forms-wizard": "^4.0.0",
    "contributte/http": "^0.4",
    "contributte/security": "^0.5",
    "contributte/utils": "^0.7.0",
    "contributte/latte": "^0.6",
    "contributte/tracy": "^0.6",
    "contributte/recaptcha": "^4.0",

    "nette/forms": "3.1.14 as 3.1.12"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/dev": "^0.5",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
